{
  "name": "q",
  "version": "1.0.0",
  "description": "Quantum Website",
  "default": "Landing/index.html",
  "scripts": {
    "watch:landing": "node-sass Landing/sass/main.scss Landing/style.css -w",
    "watch:more": "node-sass More/sass/main.scss More/style.css -w",
    "watch:team": "node-sass Team/sass/main.scss Team/style.css -w",
    "watch:training": "node-sass Training/sass/main.scss Training/style.css -w",
    "compile:css": "npm-run-all --parallel watch:landing watch:more watch:team watch:training"
  },
  "author": "Khadijah Abdulazeez",
  "license": "ISC",
  "devDependencies": {
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5"
  }
}
